{
  "key": "",
  "manifest_version": 3,
  "name": "TunePort - YouTube to Spotify",
  "version": "2.2.3",
  "description": "Sync YouTube videos to Spotify and download lossless audio.",
  "permissions": [
    "contextMenus",
    "storage",
    "activeTab",
    "tabs",
    "scripting",
    "notifications",
    "downloads",
    "identity"
  ],
  "host_permissions": [
    "https://youtube.com/*",
    "https://music.youtube.com/*",
    "https://www.youtube.com/*",
    "https://accounts.spotify.com/*",
    "https://api.spotify.com/*",
    "https://cobalt.tools/*",
    "https://*.cobalt.tools/*",
    "https://cobalt-api.meowing.de/*",
    "https://cobalt-backend.canine.tools/*",
    "https://kityune.imput.net/*",
    "https://blossom.imput.net/*",
    "https://lucida.to/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup/index.html",
    "default_title": "TunePort - Add to Spotify",
    "default_icon": {
      "16": "assets/icon-16.png",
      "32": "assets/icon-32.png",
      "48": "assets/icon-48.png",
      "128": "assets/icon-128.png"
    }
  },
  "options_ui": {
    "page": "settings/index.html",
    "open_in_tab": false
  },
  "icons": {
    "16": "assets/icon-16.png",
    "32": "assets/icon-32.png",
    "48": "assets/icon-48.png",
    "128": "assets/icon-128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["popup/auth-callback.html", "popup/auth-success.html", "assets/*"],
      "matches": ["<all_urls>"]
    }
  ]
}
