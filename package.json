{
  "name": "tuneflow",
  "version": "2.0.0",
  "description": "Easily add YouTube videos to Spotify playlists with one click",
  "main": "index.js",
  "scripts": {
    "dev": "cd tuneflow-extension && npm run dev",
    "build": "cd tuneflow-extension && npm run build",
    "package": "cd tuneflow-extension && npm run package",
    "install:extension": "cd tuneflow-extension && npm install",
    "lint": "cd tuneflow-extension && npm run lint",
    "lint:fix": "cd tuneflow-extension && npm run lint:fix",
    "test": "cd tuneflow-extension && npm test",
    "type-check": "cd tuneflow-extension && npm run type-check"
  },
  "keywords": [
    "youtube",
    "spotify",
    "music",
    "playlist",
    "extension",
    "browser-extension",
    "chrome-extension"
  ],
  "author": "TuneFlow Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/tuneflow.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/tuneflow/issues"
  },
  "homepage": "https://github.com/your-username/tuneflow#readme"
}
